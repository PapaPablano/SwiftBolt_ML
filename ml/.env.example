# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here
DATABASE_URL=postgresql://postgres:password@db.your-project.supabase.co:5432/postgres

# ML Configuration
FORECAST_HORIZONS=["1D", "1W"]
MIN_BARS_FOR_TRAINING=100
USE_ENSEMBLE_FORECASTER=true
CONFIDENCE_THRESHOLD=0.6

# Enhanced Ensemble (5-model architecture: RF, GB, ARIMA-GARCH, Prophet, LSTM)
# Set to true to enable 5-model ensemble with uncertainty quantification
ENABLE_ENHANCED_ENSEMBLE=true

# Job Configuration
SYMBOLS_TO_PROCESS=["AAPL", "SPY", "TSLA", "NVDA", "MSFT", "GOOGL", "AMZN", "META"]
BATCH_SIZE=10

# Logging
LOG_LEVEL=INFO
